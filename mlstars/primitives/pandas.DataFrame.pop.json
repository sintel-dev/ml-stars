{
    "name": "pandas.DataFrame.pop",
    "contributors": [
        "Carles Sala <csala@csail.mit.edu>"
    ],
    "documentation": "https://pandas.pydata.org/pandas-docs/stable/reference/api/pandas.DataFrame.pop.html",
    "description": "Return item and drop from frame.",
    "classifiers": {
        "type": "helper",
        "subtype": "fitting"
    },
    "modalities": [],
    "primitive": "pandas.DataFrame.pop",
    "produce": {
        "args": [
            {
                "name": "X",
                "keyword": "self",
                "type": "pandas.DataFrame"
            },
            {
                "name": "item",
                "type": "str",
                "description": "Label of column to be popped."
            }
        ],
        "output": [
            {
                "name": "item",
                "type": "pandas.Series"
            }
        ]
    },
    "hyperparameters": {
        "fixed": {}
    }
}
